<%- include("_header") -%>

<h1>Edit Job Openings</h1>

<form action="/edit/<%= model.job_id %>" method="post">
    <div class="form_horizontal">
      <%- include("_editor") -%>
      <div class="form-group row">
        <label class="col-form-label col-sm-2"></label>
        <div class="col-sm-10">
          <input type="submit" name="" value="Update" class="btn btn-default btn-warning" >
          <a class="btn btn-outline-dark cancel" href="/jobs">Cancel</a>
        </div>
      </div>
    </div>
</form>

<%- include("_footer") -%>
